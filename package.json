{
  "name": "midnightmeals",
  "version": "1.0.0",
  "description": "Applicatie waar ziekenhuismedewerkers nachtmaaltijden mee kunnen bestellen.",
  "main": "src/index.js",
  "repository": "git@github.com:Web-IV/2324-webservices-BrechtVandekerckhove.git",
  "author": "Brecht Vandekerckhove brecht.vandekerckhove@student.hogent.be",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "env-cmd nodemon"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@prisma/client": "^5.5.2",
    "config": "^3.3.9",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "prisma": "^5.5.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "nodemon": "^3.0.1"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  }
}
